{
    "name": "codicastudio/admin-backup",
    "description": "A noobs worthless garbage, just learning.",
    "keywords": [
        "worthless garabage by a noob."
    ],
    "homepage": "https://github.com/codicastudio/admin-backup",
    "license": "MIT",
    "authors": [
        {
            "name": "Michael J Brown",
            "email": "codicastudiodev@gmail.com",
            "homepage": "https://codicastudio.dev",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4.0",
        "illuminate/support": "^7.0",
        "laravel/nova": "^3.0",
        "nesbot/carbon": "^2.31",
        "codicastudio/laravel-backup": "^1.0"
    },
    "require-dev": {
        "orchestra/testbench": "^5.0",
        "phpunit/phpunit": "9.0",
        "spatie/phpunit-snapshot-assertions": "^3.0"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://nova.laravel.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "codicastudio\\BackupTool\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "codicastudio\\BackupTool\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "codicastudio\\BackupTool\\BackupToolServiceProvider"
            ]
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
